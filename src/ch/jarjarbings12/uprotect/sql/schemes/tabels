CREATE TABLE IF NOT EXISTS RP (WORLD VARCHAR NOT NULL,
                                         X INTEGER NOT NULL,
                                         Z INTEGER NOT NULL,
                                         REGION VARCHAR);

CREATE TABLE IF NOT EXISTS PR (WORLD VARCHAR NOT NULL,
                                             X INTEGER NOT NULL,
                                             Z INTEGER NOT NULL,
                                             REGION VARCHAR NOT NULL,
                                             PROTECTION BINARY NOT NULL);

CREATE TABLE IF NOT EXISTS LC (UUID VARCHAR NOT NULL,
                               LASTCONNECT INTEGER NOT NULL);